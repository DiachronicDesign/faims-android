<?xml version="1.0" ?>
<?xml-stylesheet type="text/xsl" href="sampleDataXML.xsl"?>
<dataSchema name="LogicTest" preparer="Nobody">

  <RelationshipElement name="AboveBelow" type="hierarchy">
    <description>
      Indicates that one element is above or below another element.
    </description>
    <parent>
      Above
    </parent>
    <child>
      Below
    </child>
     <property type="string" name="name" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
  </RelationshipElement>
  
  <RelationshipElement name="NextTo" type="container">
    <description>
      Indicates that one element is next to another element.
    </description>
     <property type="string" name="name" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
  </RelationshipElement>
  
  <RelationshipElement name="Similar" type="bidirectional">
    <description>
      Indicates that one element similar to another element.
    </description>
     <property type="string" name="name" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
  </RelationshipElement>
  
  <ArchaeologicalElement type="Simple">
    <description>
      An simple entity
    </description>
    <property type="string" name="name" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
    <property type="integer" name="value" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
    <property type="date" name="date" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
    <property type="time" name="time" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
    <property type="checklist" name="type" minCardinality="1" maxCardinality="1">
	  <bundle>DOI</bundle>
      <lookup>
        <term>{typeA}</term>
        <term>{typeB}</term>
        <term>{typeC}</term>
        <term>{typeD}</term>
      </lookup>
    </property>
    <property type="dropdown" name="location" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
      <lookup>
        <term>{locationA}</term>
        <term>{locationB}</term>
        <term>{locationC}</term>
        <term>{locationD}</term>
      </lookup>
    </property>
    <property type="radiogroup" name="supervisor" minCardinality="1" maxCardinality="1">
      <bundle>DOI</bundle>
    </property>
  </ArchaeologicalElement>
  
</dataSchema>
